//https://leetcode.com/problems/find-all-numbers-disappeared-in-an-array/
//https://www.youtube.com/watch?v=x6X_v_LJ-VU
class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& a) {
        for (int x : a) {
            int i = abs(x) - 1;
            if (a[i] > 0) {
                a[i] = -a[i];
            }
        }
        vector<int> ans;
        for (int v = 1; v <= (int)a.size(); v++) {
            int i = v - 1;
            if (a[i] > 0) {
                ans.push_back(v);
            }
        }
        return ans;
    }
};
